# 生产环境配置

# 后端API地址 - 使用相对路径通过Nginx代理
VITE_BACKEND_URL=

# API超时时间（毫秒）- 3分钟用于AI处理
VITE_API_TIMEOUT=180000

# 应用名称
VITE_APP_NAME=Ticketradar

# 应用版本
VITE_APP_VERSION=2.0.0

# 环境
VITE_APP_ENV=production

# 是否启用调试模式
VITE_DEBUG=false

# Google Analytics ID (可选)
VITE_GA_ID=

# 地图API密钥 (可选)
VITE_MAP_API_KEY=
