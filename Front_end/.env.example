# 前端开发环境配置模板

# 开发服务器端口
PORT=30000

# API密钥 (请在本地的 .env 文件中配置)
REACT_APP_API_KEY=

# 后端API地址
REACT_APP_BACKEND_URL=http://localhost:38181

# API超时时间（毫秒）- 3分钟用于AI处理
REACT_APP_API_TIMEOUT=180000

# 应用名称
REACT_APP_NAME=Ticketradar

# 应用版本
REACT_APP_VERSION=2.0.0

# 环境
REACT_APP_ENV=development

# 是否启用调试模式
REACT_APP_DEBUG=true

# Google Analytics ID (可选)
REACT_APP_GA_ID=

# 地图API密钥 (可选)
REACT_APP_MAP_API_KEY=
